<li id="entry-<%= entry.id %>">
  <%= link_to avatar_for(entry.user), entry.user %>
  <span class="user">
    <%= link_to entry.user.name, entry.user%>
  </span>
  <span class="content">
    <%= entry.content %>
  </span>
  <span class="timestamp">
    Posted <%= time_ago_in_words(entry.created_at) %> ago.
  </span>
</li>
